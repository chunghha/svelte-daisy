{
  "name": "svelte-daisy",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --cache --write --plugin-search-dir=. \"**/*.{cjs, js, ts, svelte}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.ts,.svelte",
    "test": "vitest",
    "test:browser": "playwright test",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint && yarn coverage"
  },
  "devDependencies": {
    "@fontsource/inter": "^4.5.11",
    "@fontsource/poppins": "^4.5.8",
    "@fontsource/space-mono": "^4.5.8",
    "@playwright-testing-library/test": "^4.3.0",
    "@playwright/test": "^1.24.2",
    "@sveltejs/adapter-auto": "^1.0.0-next.64",
    "@sveltejs/kit": "^1.0.0-next.400",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "~5.31.0",
    "@typescript-eslint/parser": "~5.31.0",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "daisyui": "^2.20.0",
    "eslint": "~8.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.0",
    "playwright-testing-library": "^4.3.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.7",
    "tslib": "^2.3.1",
    "typescript": "~4.7.4",
    "vitest": "^0.20.2",
    "vitest-svelte-kit": "^0.0.6"
  },
  "type": "module"
}

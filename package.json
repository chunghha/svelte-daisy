{
  "name": "svelte-daisy",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --plugin-search-dir=. \"**/*.{cjs, js, ts, svelte}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.ts,.svelte",
    "test": "vitest",
    "test:browser": "playwright test",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint && yarn coverage"
  },
  "devDependencies": {
    "@playwright-testing-library/test": "^4.2.1",
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-auto": "^1.0.0-next.50",
    "@sveltejs/kit": "^1.0.0-next.350",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "~5.27.1",
    "@typescript-eslint/parser": "~5.27.1",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "daisyui": "^2.15.3",
    "eslint": "~8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^19.0.0",
    "playwright-testing-library": "^4.2.1",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.2",
    "tslib": "^2.3.1",
    "typescript": "~4.7.3",
    "vitest": "^0.14.2",
    "vitest-svelte-kit": "^0.0.6"
  },
  "type": "module"
}

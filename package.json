{
  "name": "svelte-daisy",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --cache --write --plugin-search-dir=. \"**/*.{cjs, js, ts, svelte}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.ts,.svelte",
    "test": "vitest",
    "test:browser": "playwright test",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.9",
    "@fontsource/inter": "^4.5.12",
    "@fontsource/poppins": "^4.5.9",
    "@playwright-testing-library/test": "^4.3.1",
    "@playwright/test": "^1.25.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.71",
    "@sveltejs/kit": "^1.0.0-next.474",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "~5.36.2",
    "@typescript-eslint/parser": "~5.36.2",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "daisyui": "^2.25.0",
    "eslint": "~8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.0",
    "playwright-testing-library": "^4.3.1",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "svelte": "^3.50.0",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "typescript": "~4.8.2",
    "vitest": "^0.23.1",
    "vitest-svelte-kit": "^0.0.7"
  },
  "type": "module"
}

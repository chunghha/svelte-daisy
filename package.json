{
  "name": "svelte-daisy",
  "version": "0.2.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --cache --write --plugin-search-dir=. \"**/*.{cjs, js, ts, svelte}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.ts,.svelte",
    "test": "vitest",
    "test:browser": "playwright test",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint"
  },
  "dependencies": {
    "@tanstack/svelte-query": "^4.29.19"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "5.0.5",
    "@fontsource/inter": "^5.0.5",
    "@fontsource/poppins": "^5.0.5",
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.35.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^4.0.3",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "~5.61.0",
    "@typescript-eslint/parser": "~5.61.0",
    "autoprefixer": "^10.4.14",
    "c8": "^8.0.0",
    "daisyui": "^3.2.1",
    "eslint": "~8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.32.2",
    "jsdom": "^22.1.0",
    "playwright-testing-library": "^4.5.0",
    "postcss": "^8.4.25",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.5",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.6.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.2",
    "vitest": "^0.33.0"
  },
  "type": "module"
}

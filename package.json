{
  "name": "svelte-daisy",
  "version": "0.3.0",
  "engines": {
    "node": ">=20.17.0 <21.0.0",
    "pnpm": ">=10.0.0 <11.0.0"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "pnpm dlx @biomejs/biome check --write ./src",
    "lint:fix": "pnpm dlx @biomejs/biome check --write ./src",
    "test": "vitest",
    "test:browser": "playwright test",
    "coverage": "vitest run --coverage",
    "check:all": "pnpm lint:fix"
  },
  "dependencies": {
    "@tanstack/svelte-query": "^5.80.6",
    "@tanstack/svelte-query-devtools": "^5.80.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@fontsource/fira-mono": "^5.2.6",
    "@fontsource/mulish": "^5.2.6",
    "@fontsource/tienne": "^5.2.6",
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.3",
    "@sveltejs/vite-plugin-svelte": "~5.0.3",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@types/jest": "^29.5.14",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "daisyui": "~5.0.43",
    "jsdom": "^26.1.0",
    "playwright-testing-library": "^4.5.0",
    "postcss": "^8.5.4",
    "postcss-load-config": "^6.0.1",
    "svelte": "~5.28.7",
    "svelte-check": "^4.2.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.8",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "vite": "~6.3.5",
    "vitest": "^3.2.3"
  }
}
